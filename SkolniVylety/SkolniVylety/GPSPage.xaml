<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SkolniVylety.GPSPage">
    <ContentPage.Content>
        <StackLayout Margin="20">
            <ScrollView>
                <StackLayout x:Name="sSeznam"/>
            </ScrollView>
            <Button x:Name="bPridat" Text="Přidat záznam" Margin="20"
                    HorizontalOptions="End" VerticalOptions="End" Clicked="bPridat_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>